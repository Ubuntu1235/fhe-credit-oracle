{
  "version": 2,
  "name": "fhe-credit-oracle",
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_APP_NAME": "FHE Credit Oracle",
    "VITE_APP_DESCRIPTION": "Confidential Credit Scoring Protocol"
  }
}